<local>
	<outputs>
		<output id="0">
			<!--Check network connection-->
			<title>Tarkista verkkoyhteys.</title>
			<!--Unable to check for software updates at this time!-->
			<message>Ohjelmistopäivitysten tarkistus ei onnistu tällä hetkellä!</message>
		</output>
		<output id="1">
			<!--No internet connction-->
			<title>Ei internet yhteyttä.</title>
			<!--Updates could not be verified-->
			<message>Päivityksiä ei voitu varmentaa.</message>
		</output>
		<output id="2">
			<!--{App} will be updated-->
			<title>{0} päivitetään!</title>
			<!--{v1.0} to {v2.0}-->
			<message>{0} versioon {1}</message>
		</output>
		<output id="3">
			<!--{App} updated-->
			<title>{0} päivitetty.</title>
			<!--Installed version: {v1.0}-->
			<message>Asennettu versio: {0}</message>
		</output>
		<output id="4">
			<!--{App} could not be updated-->
			<title>{0} ei voitu päivittää!</title>
			<!--Please contact support-->
			<message>Ole yhteydessä tukeen.</message>
		</output>
		<output id="5">
			<!--Logs are not available yet-->
			<message>Logs are not available yet!</message>
		</output>
		<output id="6">
			<!--Starting a manual check for updated apps-->
			<message>Starting a manual check for updated apps...</message>
		</output>
		<output id="7">
			<!--Couldn't start a manual check for updated apps-->
			<message>Couldn't start a manual check for updated apps!</message>
		</output>
	</outputs>
</local>
